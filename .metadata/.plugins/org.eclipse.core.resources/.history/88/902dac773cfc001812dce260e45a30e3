package com.bridgelabz.week3;

public class AnagramArray {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int m = 0, n = 1000;
		int k = 0; int i;
		int intArray[] = new int[k];
		for(i = m; i < n; i++) {
			boolean status = true;
			for(int j = 2; j < n; j++) {
				if(i % j == 0) {
					status = false;
				}else {
					status = true;
				}
				if(status) {
					if(i!=1 || i!=0) {
					    System.out.println(i + " ");
					    intArray[k] = i;
					    k++;
					}
				}
			}
		}
		

	}

}
