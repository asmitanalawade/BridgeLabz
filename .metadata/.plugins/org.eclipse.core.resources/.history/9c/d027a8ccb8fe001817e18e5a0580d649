package com.bridgelabz.week3;

public class PowerArray {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
          int n = Integer.parseInt(args[0]);
          int a[] = new int[n];
          int power = 1;
          int i = 0;
          while(i < n) {
        	  System.out.println("Power of 2^ " + i + " = " + power);
        	  power = power * 2;
        	  i++;
          }
	}

}
