package com.bridgelabz.week3;

import utility.Utility;

public class PalindromeArray {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int n = 1000;
		int k;
		 PalindromeArray obj=new PalindromeArray();
	        if(obj.(Utility.primeNumberPalindrome(n))==true && obj.Utility.Checkpalindrome(n,k)==true)
	            System.out.println(n+" is a Prime Palindrome i.e. a PalPrime Number");
	        else
	            System.out.println(n+" is not a PalPrime Number");
		Utility.primeNumberPalindrome(n);

	}

}
