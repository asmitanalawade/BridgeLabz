		int A[][]  = new int[m][n];
		int B[] = new int[m*n];
		
		
		System.out.println("Enter the 2D array: ");
		
		
	}
	boolean isprime(int n) {
		for(int i = 1; i < n; i++) {
			while(n % i  == 0) {
				n++;
			}
			if(n == 2) {
				return true;
			}else {
				return false;
			}
		 			
		}
		return false;
		
		}


}
